{
  "name": "bsquare",
  "version": "0.0.0",
  "description": "B Square Event Management",
  "main": "server.js",
  "scripts": {
    "jshint-node": "./node_modules/.bin/jshint ./app --exclude ./node_modules",
    "jshint-front": "./node_modules/.bin/jshint ./public/js --exclude-path ./.jshintignore",
    "babel-node": "babel-node --ignore='node_modules'",
    "forever": "forever start --uid 'bsquare-app' --minUptime 3000 --spinSleepTime 6000 -al ./bsquare-app.log -c babel-node server.js",
    "dev": "nodemon --exec npm run babel-node -- server.js",
    "server": "grunt && babel-node server.js"
  },
  "repository": "https://github.com/vhalme/bsquare.git",
  "dependencies": {
    "MD5": "^1.2.2",
    "angular-video": "0.0.2",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-transform-regenerator": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "body-parser": "^1.13.1",
    "connect-busboy": "0.0.2",
    "ejs": "^2.3.2",
    "express": "^4.13.0",
    "form-urlencoded": "^1.2.1",
    "iban": "arhs/iban.js",
    "install": "^0.6.1",
    "log4js": "^0.6.27",
    "merge": "^1.2.0",
    "method-override": "^2.3.3",
    "moment": "^2.10.3",
    "mongoose": "^4.4.12",
    "mongoose-q": "0.1.0",
    "morgan": "^1.6.0",
    "node-uuid": "^1.4.3",
    "nodemailer": "^2.3.2",
    "npm": "^3.8.6",
    "password-hash": "^1.2.2",
    "q": "^1.4.1",
    "request": "^2.58.0",
    "sendgrid": "^2.0.0",
    "traceur": "0.0.107",
    "utf8": "^2.1.0",
    "xml2js": "^0.4.9"
  },
  "devDependencies": {
    "grunt": ">=0.4.0",
    "grunt-babel": "^6.0.0",
    "grunt-concurrent": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-uglify": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-ng-annotate": "^2.0.1",
    "grunt-ngmin": "0.0.3",
    "grunt-nodemon": "^0.4.0",
    "grunt-ts": "^5.5.0-beta.2",
    "load-grunt-tasks": "^3.2.0"
  },
  "author": "Vladimir Halme",
  "license": "MIT",
  "readmeFilename": "README.md"
}
